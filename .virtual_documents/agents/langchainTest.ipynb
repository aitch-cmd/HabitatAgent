from langchain.agents import create_agent
from Prompts import root_agent_promptV2
from LlmProviders.GoogleLangchain import llm
from agent_tools import search_accommodations, add_listing

agent = create_agent(
    llm=llm,
    tools=[search_accommodations, add_listing],
    prompt=root_agent_promptV2,
    verbose=True
)


response = agent.invoke(
    {"messages": [{"role": "user", "content": "hi I need to create a listing for my apartment"}]}
)

print(response)









